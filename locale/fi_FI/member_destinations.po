# --- English text = Destinations Wishlist
msgid "headline"
msgstr "Luettelo toivekohteista"

# --- English text = Add Destinations
msgid "add_destination_headline"
msgstr "Lisää kohteita"

# --- English text = No regions available
msgid "message.country_without_regions"
msgstr "Alueita ei ole määritelty"

# --- English text = No cities available
msgid "message.country_without_cities"
msgstr "Aluevalinta ei ole mahdollinen"

# --- English text = My Preferred Destinations
msgid "preferred_destinations_headline"
msgstr "Haluamani kohteet"

# --- English text = Delete entry
msgid "delete_entry"
msgstr "Tyhjennä lomake"

# --- English text = 1st step: Choose a Country
msgid "step_one.headline"
msgstr "Vaihe 1: Valitse maa"

# --- English text = Now, click the Add Country button if you are generally interested in visiting this country.
msgid "step_one.text"
msgstr "Paina Lisää maa -näppäintä, jos olet olet kiinnostunut vierailusta tähän maahan."

# --- English text = 2nd Step: Choose a Region
msgid "step_two.add_region_headline"
msgstr "Vaihe 2: Valitse alue"

# --- English text = and click the Add Region button if you are interested in visiting anywhere within this region.
msgid "step_two.add_region_text"
msgstr "ja paina Lisää alue -näppäintä, jos ole kiinnostunut vierailemaan tällä alueella."

# --- English text = Or Alternately: Choose a City
msgid "step_two.add_city_headline"
msgstr "Vaihtoehto: Valitse kaupunki"

# --- English text = and click the Add City button if you are very specifically interested in exchanging with members who live near this city.
msgid "step_two.add_city_text"
msgstr "ja paina Lisää kaupunki -näppäintä, jos olet erityisen kiinnostunut vaihdosta jäsenen kanssa, joka asuu tämän kaupungin läheisyydessä."

